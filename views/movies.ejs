        nav{
            display: flex;
            flex-direction: row;
            align-items: center;
            justify-content: space-between;
            

            position: sticky;
            z-index: 1;

            background-color: inherit;
            width: 100vw;

            top: 0px;

            padding: 16px 8px;

            border-radius: 10px;

        }

        .profileLogo{
            display: flex;
            flex-direction: row;

            background-color: transparent;

            height: 100%;
            width: auto;

        }
        .profileLogo button {
            background-color: slateblue;

            color: white;
            
            padding: 10px;

            margin-right: 30px;

            border-radius: 5px;

            box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.567);
            outline: none;
            border: none;

        }
        .profileLogo button a {
            color: inherit;
            text-decoration: none;
            font-size: 1rem;
        }

        nav> h1 {
            font-size: 30px;
        }

        body>:nth-child(2){


            /* margin-top: 14vh; */
        }















































        body{
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: space-between;

            background-color: black;

            overflow: hidden;

            width: 100vw;
            height: 100vh;
        }


        main{
            display: flex;
            flex-direction: row;
            align-items: center;
            justify-content: space-between;
            
            background-color: transparent;
            
            width: 100%;
            height: 100vh;

        }
        
        .panel{
            display: flex;
            flex-direction: column;
            align-items: flex-start;
            justify-content: flex-start;

            position: relative;

            background-color: rgb(27, 27, 27);
            
            width: 70px;
            height: 100%;

            padding: 20px 0px;

            border-radius: 10px;
            perspective: 1000px;
            
            overflow: hidden;
        }
        .panel .btn1{
            display: flex;
            align-items: center;
            justify-content: center;
            flex-wrap: wrap;
            text-wrap: wrap;

            background-color: rgb(100, 100, 100);
            box-shadow: 0px 10px 10px rgba(0, 0, 0, 0.623);

            max-width: 100%;
            height: auto;
            
            padding: 25px 5px;
            margin: 5px 0px;

            font-size: 13px;
            font-weight: 700;
        }
        .center{
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: flex-start;

            background-color: rgb(27, 27, 27);

            border-radius: 10px;

            width: 100%;
            height: 100%;

            margin: 5px;
            padding: 10px;

            overflow-y: auto;
        }

        .center section{
            display: flex;
            flex-direction: row;
            flex-wrap: wrap;
            align-items: center;
            justify-content: space-evenly;

            background-color: transparent;
            width: 100%;
        }
        














































        .movieContainer{
            display: flex;
            flex-direction: row;
            flex-wrap: wrap;
            align-items: center;
            justify-content: space-evenly;

            background-color: transparent;

            width: 100%;

            padding: 2px;

        }

        .movieCard{
            display: flex;
            flex-direction: column;
            align-items: center;

            position: relative;
            background-color: transparent;

            border: 2px solid transparent;
            border-radius: 15px;

            width: 200px;
            height: 260px;

            margin: 14px;

            overflow: hidden;
            box-shadow: 20px 20px 30px -5px rgba(0, 0, 0, 0.427);

            &:hover{
                box-shadow: 40px 40px 40px -20px rgba(0, 0, 0, 0.87);
                border-color: rgba(219, 219, 219, 0.732);
                transform: scale(1.01);
            }
        }

        .movieCard img{

            width: 100%;
            height: 100%;
            object-fit: fill;
        }

        .text{
            display: flex;
            flex-direction: column-reverse;
            flex-wrap: wrap;
            align-items: center;
            text-align: center;
            
            position: absolute;
            background: linear-gradient(to top, rgb(0, 0, 0)10%, rgba(253, 253, 253, 0)90%);

            width: 100%;
            height: inherit;

            padding: 6px;

            text-shadow: 2px 1px 3px black;
        }
        .movieCard a{
            position: absolute;
            bottom: 0;
            width: 100%;
            height: inherit;

            background-color: transparent;

        }


        
    </style>
